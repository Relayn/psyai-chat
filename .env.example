# Django Core Settings
# ВАЖНО: Для production сгенерируйте новый ключ!
DJANGO_SECRET_KEY='change-me-to-a-super-secret-and-long-key'
DJANGO_DEBUG=True

# Database Settings
# Пример для PostgreSQL: postgresql://<USER>:<PASSWORD>@<HOST>:<PORT>/<DB_NAME>
# Пример для локального Docker: postgresql://psyai_user:psyai_password@db:5432/psyai_db
DATABASE_URL='postgresql://psyai_user:psyai_password@db:5432/psyai_db'

# Celery Settings
# URL для подключения к брокеру сообщений Redis
CELERY_BROKER_URL='redis://redis:6379/0'

# External Services
GPT_API_KEY='your-gpt-api-key-goes-here'